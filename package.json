  "build": {
    "appId": "com.prysm.app",
    "productName": "Prysm",
    "artifactName": "${productName}-${os}-${arch}-${version}.${ext}",
    "directories": {
      "output": "release",
      "buildResources": "images"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      ".env.production",
      "app-update.yml"
    ],
    "extraResources": [
      {
        "from": ".env.production",
        "to": ".env.production"
      },
      {
        "from": "app-update.yml",
        "to": "app-update.yml"
      }
    ],
    "mac": {
      "category": "public.app-category.business",
      "icon": "images/prysm-icon-1024.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "images/prysm-icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false,
      "installerIcon": "images/prysm-icon.ico",
      "uninstallerIcon": "images/prysm-icon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "icon": "images/prysm-icon.png",
      "category": "Office",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    },
    "publish": {
      "provider": "github",
      "owner": "daniel5426",
      "repo": "opticai",
      "private": true
    }
  }
}
